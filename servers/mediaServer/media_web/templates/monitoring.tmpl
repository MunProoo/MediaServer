    <script src="https://cdn.jsdelivr.net/npm/chart.js@3.9.1/dist/chart.min.js"></script>
    <script>
        let cpuChart, memoryChart, networkChart;

        // ì°¨íŠ¸ ì´ˆê¸°í™”
        function initCharts() {
            const chartOptions = {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: {
                        display: false
                    }
                },
                scales: {
                    y: {
                        beginAtZero: true,
                        max: 100,
                        grid: {
                            color: 'rgba(255, 255, 255, 0.1)'
                        },
                        ticks: {
                            color: '#e4e6eb'
                        }
                    },
                    x: {
                        grid: {
                            color: 'rgba(255, 255, 255, 0.1)'
                        },
                        ticks: {
                            color: '#e4e6eb'
                        }
                    }
                }
            };

            // CPU ì°¨íŠ¸
            const cpuCtx = document.getElementById('cpuChart').getContext('2d');
            cpuChart = new Chart(cpuCtx, {
                type: 'line',
                data: {
                    labels: [],
                    datasets: [{
                        label: 'CPU %',
                        data: [],
                        borderColor: '#4fc3f7',
                        backgroundColor: 'rgba(79, 195, 247, 0.1)',
                        tension: 0.4,
                        fill: true
                    }]
                },
                options: chartOptions
            });

            // ë©”ëª¨ë¦¬ ì°¨íŠ¸
            const memoryCtx = document.getElementById('memoryChart').getContext('2d');
            memoryChart = new Chart(memoryCtx, {
                type: 'line',
                data: {
                    labels: [],
                    datasets: [{
                        label: 'Memory %',
                        data: [],
                        borderColor: '#9c27b0',
                        backgroundColor: 'rgba(156, 39, 176, 0.1)',
                        tension: 0.4,
                        fill: true
                    }]
                },
                options: chartOptions
            });

            // ë„¤íŠ¸ì›Œí¬ ì°¨íŠ¸
            const networkCtx = document.getElementById('networkChart').getContext('2d');
            networkChart = new Chart(networkCtx, {
                type: 'line',
                data: {
                    labels: [],
                    datasets: [{
                        label: 'Network MB/s',
                        data: [],
                        borderColor: '#4caf50',
                        backgroundColor: 'rgba(76, 175, 80, 0.1)',
                        tension: 0.4,
                        fill: true
                    }]
                },
                options: {
                    ...chartOptions,
                    scales: {
                        ...chartOptions.scales,
                        y: {
                            ...chartOptions.scales.y,
                            max: undefined,
                            ticks: {
                                ...chartOptions.scales.y.ticks,
                                callback: function(value) {
                                    return value.toFixed(2) + ' MB/s';
                                }
                            }
                        }
                    }
                }
            });
        }

        // ë°ì´í„° ì—…ë°ì´íŠ¸
        async function updateMonitoringData() {
            try {
                const response = await fetch('/api/monitoring/data?history=true');
                const data = await response.json();

                // ì‹œìŠ¤í…œ ë©”íŠ¸ë¦­ ì—…ë°ì´íŠ¸
                updateSystemMetrics(data.system);
                
                // ìš”ì•½ ì •ë³´ ì—…ë°ì´íŠ¸
                updateSummary(data.summary);
                
                // ìŠ¤íŠ¸ë¦¼ ì •ë³´ ì—…ë°ì´íŠ¸
                updateStreams(data.streams);
                
                // ê²½ê³  ì—…ë°ì´íŠ¸
                updateAlerts(data.alerts);
                
                // íˆìŠ¤í† ë¦¬ ì°¨íŠ¸ ì—…ë°ì´íŠ¸
                if (data.history) {
                    updateCharts(data.history);
                }

                // ë§ˆì§€ë§‰ ì—…ë°ì´íŠ¸ ì‹œê°„
                const lastUpdate = new Date(data.last_update);
                document.getElementById('last-update').textContent = 
                    `ë§ˆì§€ë§‰ ì—…ë°ì´íŠ¸: ${lastUpdate.toLocaleTimeString('ko-KR')}`;

            } catch (error) {
                console.error('Error fetching monitoring data:', error);
            }
        }

        // ì‹œìŠ¤í…œ ë©”íŠ¸ë¦­ ì—…ë°ì´íŠ¸
        function updateSystemMetrics(system) {
            // CPU
            const cpuPercent = system.cpu_usage_percent.toFixed(1);
            document.getElementById('cpu-usage').textContent = cpuPercent + '%';
            document.getElementById('cpu-progress').style.width = cpuPercent + '%';
            document.getElementById('cpu-progress').textContent = cpuPercent + '%';
            document.getElementById('cpu-progress').className = 
                'progress-bar ' + getProgressColor(cpuPercent);
            document.getElementById('cpu-cores').textContent = 
                `${system.cpu_cores} ì½”ì–´`;

            // Memory
            const memPercent = system.memory_usage_percent.toFixed(1);
            document.getElementById('memory-usage').textContent = memPercent + '%';
            document.getElementById('memory-progress').style.width = memPercent + '%';
            document.getElementById('memory-progress').textContent = memPercent + '%';
            document.getElementById('memory-progress').className = 
                'progress-bar ' + getProgressColor(memPercent);
            document.getElementById('memory-info').textContent = 
                `${(system.memory_used_mb / 1024).toFixed(1)} GB / ${(system.memory_total_mb / 1024).toFixed(1)} GB`;

            // Disk
            const diskPercent = system.disk_used_percent.toFixed(1);
            document.getElementById('disk-usage').textContent = diskPercent + '%';
            document.getElementById('disk-progress').style.width = diskPercent + '%';
            document.getElementById('disk-progress').textContent = diskPercent + '%';
            document.getElementById('disk-progress').className = 
                'progress-bar ' + getProgressColor(diskPercent);
            document.getElementById('disk-info').textContent = 
                `${(system.disk_free_mb / 1024).toFixed(1)} GB ë‚¨ìŒ / ${(system.disk_total_mb / 1024).toFixed(1)} GB`;

            // Network (ì´ˆë‹¹ ì „ì†¡ëŸ‰)
            const totalNetwork = system.network_sent_mbps + system.network_recv_mbps;
            document.getElementById('network-usage').textContent = 
                `${totalNetwork.toFixed(2)} MB/s`;
            document.getElementById('network-sent').textContent = 
                `${system.network_sent_mbps.toFixed(2)} MB/s`;
            document.getElementById('network-recv').textContent = 
                `${system.network_recv_mbps.toFixed(2)} MB/s`;
            document.getElementById('goroutines').textContent = 
                `GoRoutines: ${system.goroutines}`;

            // Uptime
            document.getElementById('uptime').textContent = `ì—…íƒ€ì„: ${system.uptime}`;
        }

        // ìš”ì•½ ì •ë³´ ì—…ë°ì´íŠ¸
        function updateSummary(summary) {
            document.getElementById('total-streams').textContent = summary.total_streams;
            document.getElementById('online-streams').textContent = summary.online_streams;
            document.getElementById('total-clients').textContent = summary.total_clients;
            document.getElementById('recording-channels').textContent = summary.recording_channels;
        }

        // ìŠ¤íŠ¸ë¦¼ ì •ë³´ ì—…ë°ì´íŠ¸
        function updateStreams(streams) {
            const tbody = document.getElementById('streams-tbody');
            tbody.innerHTML = '';

            if (streams.length === 0) {
                tbody.innerHTML = '<tr><td colspan="7" class="text-center">ìŠ¤íŠ¸ë¦¼ì´ ì—†ìŠµë‹ˆë‹¤</td></tr>';
                return;
            }

            streams.forEach(stream => {
                Object.entries(stream.channels).forEach(([channelId, channel]) => {
                    const row = document.createElement('tr');
                    
                    const statusBadge = channel.status === 'online' 
                        ? '<span class="badge badge-online">ì˜¨ë¼ì¸</span>'
                        : '<span class="badge badge-offline">ì˜¤í”„ë¼ì¸</span>';
                    
                    const recordingBadge = channel.is_recording
                        ? '<span class="badge badge-recording"><span class="icon icon-circle"></span> ë…¹í™”ì¤‘</span>'
                        : '-';

                    row.innerHTML = `
                        <td>${stream.stream_name}</td>
                        <td>ì±„ë„ ${channelId}</td>
                        <td>${statusBadge}</td>
                        <td>${channel.client_count}</td>
                        <td>${recordingBadge}</td>
                        <td>${channel.uptime || '-'}</td>
                        <td><small class="text-muted">${channel.url.substring(0, 50)}...</small></td>
                    `;
                    
                    tbody.appendChild(row);
                });
            });
        }

        // ê²½ê³  ì—…ë°ì´íŠ¸
        function updateAlerts(alerts) {
            const container = document.getElementById('alerts-container');
            container.innerHTML = '';

            if (!alerts || alerts.length === 0) return;

            alerts.forEach(alert => {
                const alertClass = `alert-${alert.level}`;
                const alertDiv = document.createElement('div');
                alertDiv.className = `alert ${alertClass} alert-dismissible fade show`;
                alertDiv.innerHTML = `
                    <strong>${alert.level.toUpperCase()}:</strong> ${alert.message}
                    <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                `;
                container.appendChild(alertDiv);
            });
        }

        // ì°¨íŠ¸ ì—…ë°ì´íŠ¸
        function updateCharts(history) {
            // CPU ì°¨íŠ¸
            cpuChart.data.labels = history.cpu_history.map(p => 
                new Date(p.timestamp).toLocaleTimeString('ko-KR', {hour: '2-digit', minute: '2-digit'})
            );
            cpuChart.data.datasets[0].data = history.cpu_history.map(p => p.value);
            cpuChart.update('none');

            // ë©”ëª¨ë¦¬ ì°¨íŠ¸
            memoryChart.data.labels = history.memory_history.map(p => 
                new Date(p.timestamp).toLocaleTimeString('ko-KR', {hour: '2-digit', minute: '2-digit'})
            );
            memoryChart.data.datasets[0].data = history.memory_history.map(p => p.value);
            memoryChart.update('none');

            // ë„¤íŠ¸ì›Œí¬ ì°¨íŠ¸
            networkChart.data.labels = history.network_history.map(p => 
                new Date(p.timestamp).toLocaleTimeString('ko-KR', {hour: '2-digit', minute: '2-digit'})
            );
            networkChart.data.datasets[0].data = history.network_history.map(p => p.value);
            networkChart.update('none');
        }

        // Progress bar ìƒ‰ìƒ
        function getProgressColor(percent) {
            if (percent >= 90) return 'bg-danger';
            if (percent >= 75) return 'bg-warning';
            if (percent >= 50) return 'bg-info';
            return 'bg-success';
        }

        // ì´ˆê¸°í™”
        document.addEventListener('DOMContentLoaded', function() {
            initCharts();
            updateMonitoringData();
            
            // 5ì´ˆë§ˆë‹¤ ìë™ ì—…ë°ì´íŠ¸
            setInterval(updateMonitoringData, 5000);
        });
    </script>
